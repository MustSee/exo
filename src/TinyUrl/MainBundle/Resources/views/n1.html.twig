<!doctype html>
<html lang="fr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block title %}TinyUrl{% endblock %}</title>
    <!-- CSS -->
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/tinyurlmain/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/tinyurlmain/css/custom.css') }}">
    {% endblock %}
</head>

<body>
    {% block body %} {% endblock %}

    <!-- JAVASCRIPT -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js"></script>
    {% block javascripts %}
        <script type="text/javascript" src="{{ asset('bundles/tinyurlmain/js/bootstrap.min.js') }}"></script>
    {% endblock javascripts %}
</body>

</html><!-- Fin du NIVEAU 1 -->


